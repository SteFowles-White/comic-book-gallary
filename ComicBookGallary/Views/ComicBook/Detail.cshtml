@{
    Layout = null;



}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>@ViewBag.SeriersTitle</h1>
        <h2>Issue: @ViewBag.IssueNumber</h2>
        <h5>Description:</h5>
        <div>@Html.Raw(ViewBag.Description)</div>


        @if (ViewBag.Artists.Length > 0)
        {
        <h5>Artists:</h5>
        <div>
            <ul>
                @foreach (string item in ViewBag.Artists)
                {
                    <li>@item</li>;

                }
            </ul>
        </div>
        }

    </div>
</body>
</html>
